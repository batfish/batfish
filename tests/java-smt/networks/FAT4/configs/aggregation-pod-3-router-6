interface Loopback0
  ip address 70.0.16.0/32
!
interface Serial0
  ip address 10.0.19.1/31
!
interface Serial1
  ip address 10.0.23.1/31
!
interface Serial2
  ip address 10.0.12.0/31
!
interface Serial3
  ip address 10.0.13.0/31
!
router bgp 16
  neighbor 10.0.19.0 remote-as 0
  neighbor 10.0.23.0 remote-as 1
  neighbor 10.0.12.1 remote-as 18
  neighbor 10.0.13.1 remote-as 19
!
