net del all
#
net add hostname cumulus_nclu_invalid_vrfs
#
# lo; also 1 valid and 1 invalid
net add vrf lo,vrf1
# name of bond
net add bond bond1 bond slaves swp1
net add vrf bond1
# name of physical interface
net add vrf eth0
net add vrf swp2
# looks like subinterface
net add vrf vrf1.1000
