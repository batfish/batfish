#TMSH-VERSION: 13.1.1

sys global-settings {
    hostname f5_bigip_structured_net_routing_bgp
}

net routing bgp /Common/my_bgp {
    address-family {
        ipv4 {
            redistribute {
                kernel {
                    route-map /Common/MY_KERNEL_OUT
                }
            }
        }
        ipv6 { }
    }
    local-as 123
    neighbor {
        dead:beef::1 {
            address-family {
                ipv4 {
                    activate disabled
                }
                ipv6 {
                    capability {
                        graceful-restart disabled
                    }
                    maximum-prefix {
                        value 12345
                    }
                    route-map {
                        out /Common/MY_IPV6_OUT
                    }
                }
            }
            description "Cool IPv6 BGP neighbor description"
            fall-over bfd
            remote-as 234
            update-source SOME_VLAN
        }
        192.0.2.1 {
            address-family {
                ipv4 {
                    capability {
                        graceful-restart disabled
                    }
                    maximum-prefix {
                        value 23456
                    }
                    route-map {
                        out /Common/MY_IPV4_OUT
                    }
                }
                ipv6 {
                    activate disabled
                }
            }
            description "Cool IPv4 BGP neighbor description"
            ebgp-multihop 3
            fall-over bfd
            remote-as 456
            update-source SOME_VLAN
        }
    }
    profile /Common/bgp
    route-domain /Common/0
    router-id 192.0.2.1
    router-id dead:beef::1234
}

