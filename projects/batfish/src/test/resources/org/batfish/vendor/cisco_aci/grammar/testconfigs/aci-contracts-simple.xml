<?xml version="1.0" encoding="UTF-8"?>
<polUni>
  <fvTenant name="tenant1" descr="Simple Contract Test Tenant">
    <fvCtx name="vrf1" descr="Test VRF"/>
    <fvBD name="bd1" descr="Test Bridge Domain">
      <fvRsCtx tnFvCtxName="vrf1"/>
      <fvSubnet ip="10.0.0.0/24"/>
    </fvBD>
    <fvAEPg name="web_epg" descr="Web EPG">
      <fvRsBd tnFvBDName="bd1"/>
    </fvAEPg>
    <fvAEPg name="app_epg" descr="App EPG">
      <fvRsBd tnFvBDName="bd1"/>
    </fvAEPg>
    <vzBrCP name="web_to_app_contract" descr="Simple Web to App Contract">
      <vzSubj name="http" descr="HTTP Subject">
        <vzRsSubjFiltAtt tnVzFilterName="allow_http"/>
      </vzSubj>
    </vzBrCP>
    <vzFilter name="allow_http" descr="Allow HTTP Filter">
      <vzEntry name="allow_http" descr="HTTP Entry">
        <vzMatchTcpProt/>
        <vzMatchDstmPort name="80"/>
      </vzEntry>
    </vzFilter>
    <vzRsProtProv ip="web_epg" tnVzBrCPName="web_to_app_contract"/>
    <vzRsProtCons ip="app_epg" tnVzBrCPName="web_to_app_contract"/>
  </fvTenant>
</polUni>
