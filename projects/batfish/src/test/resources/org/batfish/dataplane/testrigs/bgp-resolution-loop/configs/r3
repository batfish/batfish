hostname r3

feature bgp

route-map PERMIT_ALL permit 10

interface Ethernet1/1
  no switchport
  ip address 10.10.10.2/30
  no shutdown

interface loopback0
  ip address 2.2.2.2/32
router bgp 65002
  router-id 2.2.2.2
  neighbor 10.10.10.1
    remote-as 65001
    address-family ipv4 unicast
      route-map PERMIT_ALL out
      default-originate
