config system global
    set hostname "firewall_policy"
end
config system interface
    edit "port1"
        set vdom "root"
        set type physical
        set ip 10.0.1.1/24
    next
    edit port2
        set vdom "root"
        set type physical
        set ip 10.0.2.1/24
    next
    edit port3
        set vdom "root"
        set type physical
        set ip 10.0.3.1/24
    next
    edit port4
        set vdom "root"
        set type physical
        set ip 10.0.4.1/24
    next
    edit port5
        set vdom "root"
        set type physical
        set ip 10.0.5.1/24
    next
end
config system zone
    edit zone1
        set interface port3
    next
    edit zone2
        set interface port4
    next
    edit zone3
        set interface port5
    next
end
config firewall service custom
    edit "custom_tcp_11"
        set category "Network Services"
        set tcp-portrange 11
    next
    edit "custom_tcp_12_from_11"
        set category "Web Access"
        set tcp-portrange 12:11
    next
    edit ALL
        set category "General"
        set protocol IP
    next
end
config firewall address
    edit addr1
        set subnet 10.0.1.0 255.255.255.0
    next
    edit addr2
        set subnet 10.0.2.0 255.255.255.0
    next
    edit all
    next
end
config firewall policy
    # Disabled rule should be ignored
    edit 0
        set uuid aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa
        set action deny
        set srcintf port1
        set dstintf port2
        set srcaddr addr1
        set dstaddr addr2
        set service custom_tcp_11
        set status disable
    next
    edit 4294967294
        set name "longest allowed firewall policy nam"
        set uuid aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa
        set srcintf port1
        set dstintf port1 port2
        set srcaddr addr1
        set dstaddr addr2
        set service custom_tcp_12_from_11
        set comments "firewall policy comments"
    next
    # Number doesn't indicate order, just a unique identifier
    edit 1
        set name "Permit Custom TCP Traffic"
        set action accept
        set srcintf port1
        set dstintf port2
        set srcaddr addr1
        set dstaddr addr2
        set service custom_tcp_11
        set status enable
        append srcintf port2
        append dstintf port1
        append srcaddr addr2
        append dstaddr addr1
        append service custom_tcp_12_from_11
    next
    edit 2
        set action accept
        set srcintf any
        set dstintf any
        set srcaddr all
        set dstaddr all
        set service ALL
    next
    edit 3
        set action accept
        set srcintf zone1 port2 zone3
        append srcintf zone2
        set dstintf zone1 zone2
        set dstintf zone1
        append dstintf zone3
        set srcaddr all
        set dstaddr all
        set service ALL
    next
end
