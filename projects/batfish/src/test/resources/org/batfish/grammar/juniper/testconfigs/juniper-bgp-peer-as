set system host-name juniper-bgp-peer-as

#
set protocols bgp group F type internal
set protocols bgp group F neighbor 1.1.1.1 description D
set protocols bgp group G type external
set protocols bgp group G neighbor 2.2.2.2 description D
set protocols bgp group G neighbor 2.2.2.2 peer-as 1
set protocols bgp group G neighbor 3.3.3.3 description D


