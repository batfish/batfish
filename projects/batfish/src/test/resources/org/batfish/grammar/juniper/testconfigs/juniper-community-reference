#
set system host-name juniper-community-reference
#
set policy-options community COMM1 members 1
set policy-options community COMM2 invert-match
set policy-options community COMM2 members 2
#
set policy-options policy-statement COMMUNITY_POLICY term T1 from community COMM1
set policy-options policy-statement COMMUNITY_POLICY term T1 from community COMM3
set policy-options policy-statement COMMUNITY_POLICY term T1 then accept
