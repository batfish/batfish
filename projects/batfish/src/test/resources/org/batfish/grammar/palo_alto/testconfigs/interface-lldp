set deviceconfig system hostname interface-lldp
set network interface ethernet ethernet1/1 layer3 lldp enable yes
set network interface ethernet ethernet1/1 layer3 ip 1.1.1.1/24
set network interface ethernet ethernet1/2 layer3 lldp enable no
set network interface ethernet ethernet1/2 layer3 ip 1.1.2.1/24
set network interface ethernet ethernet1/3 layer3 ip 1.1.3.1/24
set network interface ethernet ethernet1/4 layer3 lldp enable yes
set network interface ethernet ethernet1/4 layer3 ip 1.1.4.1/24
set network interface ethernet ethernet1/5 layer3 units ethernet1/5.1 lldp enable no
set network interface ethernet ethernet1/5 layer3 units ethernet1/5.1 ip 1.1.5.1/24
set network virtual-router default interface [ ethernet1/1 ethernet1/2 ethernet1/3 ethernet1/4 ethernet1/5 ethernet1/5.1 ]
