! This is an ASA device.
ASA Version 9.9
!
hostname asa-nat-object-dynamic
!
interface GigabitEthernet0/0
 nameif inside
 security-level 100
 ip address 1.0.0.1 255.255.255.0
!
interface GigabitEthernet0/1
 nameif outside
 security-level 0
 ip address 2.0.0.1 255.255.255.0
!
interface GigabitEthernet0/2
 nameif other
 ip address 3.0.0.1 255.255.255.0
!
object network source-real-1
 subnet 1.1.1.0 255.255.255.0
object network source-real-2
 subnet 2.2.2.0 255.255.255.0
object network source-real-3
 subnet 3.3.3.0 255.255.255.0
object network source-real-4
 subnet 4.4.4.0 255.255.255.0
object network source-mapped-1
 range 192.168.1.1 192.168.1.10
object network source-mapped-2
 range 192.168.2.1 192.168.2.10
object network source-mapped-3
 range 192.168.3.1 192.168.3.10
object network source-mapped-4
 range 192.168.4.1 192.168.4.10
!
object network source-real-1
 nat (inside,outside) dynamic source-mapped-1
object network source-real-2
 nat (outside,inside) dynamic source-mapped-2
object network source-real-3
 nat (inside,any) dynamic source-mapped-3
object network source-real-4
 nat (any,any) dynamic source-mapped-4
!
