hostname r2
!
interface FastEthernet0/0
 ip address 1.2.0.2 255.255.255.0
!
interface Loopback0
 ip address 2.2.2.2 255.255.255.255
!
router bgp 2
 bgp router-id 2.2.2.2
 redistribute static
 neighbor 1.2.0.1 remote-as 1
 neighbor 1.2.0.1 distribute-list 1 out
!
access-list 1 deny 1.2.3.4
access-list 1 permit any
!
ip route 1.2.3.4 255.255.255.255 Null0
ip route 5.6.7.8 255.255.255.255 Null0
