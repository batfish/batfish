! This is an ASA device.
ASA Version 9.9
!
hostname asa-security-level-permit-both-out-filter
!
interface GigabitEthernet0/1
  nameif name1
  security-level 50
  ip address 3.0.0.2 255.255.255.0
!
interface GigabitEthernet0/2
  nameif name2
  security-level 50
  ip address 3.0.1.2 255.255.255.0
!
interface GigabitEthernet0/3
  nameif name3
  security-level 10
  ip address 3.0.1.2 255.255.255.0
!
interface GigabitEthernet0/3
  nameif name4
  security-level 100
  ip address 3.0.1.2 255.255.255.0
!
same-security-traffic permit inter-interface
same-security-traffic permit intra-interface
!
access-group FILTER_OUT out interface name2
!
access-list FILTER_OUT extended permit tcp any any eq 123
