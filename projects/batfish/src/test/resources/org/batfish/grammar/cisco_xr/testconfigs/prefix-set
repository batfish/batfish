!RANCID-CONTENT-TYPE: cisco-xr
!
hostname prefix-set
!
prefix-set pre_ipv4
  1.2.3.4/30
end-set
prefix-set pre_ipv6
  2001::ffff:0/124
end-set
prefix-set pre_combo
  1.2.3.4/30,
  2001::ffff:0/124
end-set
route-policy rp_ip
  if destination in pre_ipv4 then
    pass
    done
  endif
  if destination in pre_ipv6 then
    pass
    done
  endif
  if destination in pre_undef then
    pass
    done
  endif
end-policy
