#
set system host-name bgp-multipath-internal
#
set routing-options autonomous-system 1
set protocols bgp group MYGROUP multipath
set protocols bgp group MYGROUP local-as 1
set protocols bgp group MYGROUP peer-as 2
set protocols bgp group MYGROUP neighbor 1.1.1.1
set protocols bgp group MYGROUP enforce-first-as
set protocols bgp group MYGROUP type internal
