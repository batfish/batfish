!RANCID-CONTENT-TYPE: cisco-xr
!
hostname bgp-aggregate
!
router static
  address-family ipv4 unicast
    1.1.1.0/24 Null0
    2.2.2.0/24 Null0
!
router bgp 1
  bgp router-id 1.1.1.1
  address-family ipv4 unicast
    aggregate-address 1.1.0.0/16
    aggregate-address 2.2.0.0/16 summary-only
    redistribute static
!
