config system global
    set hostname "iface"
end
config system interface
    edit "port1"
        set vdom "root"
        # mask format
        set ip 192.168.122.2 255.255.255.0
        set type physical
        set alias "longest possibl alias str"
        set description "quoted description w/ spaces and more"
        set snmp-index 1
        set secondary-IP enable
        config secondaryip
            edit 4294967295
                set ip 10.0.1.1/24
            next
            edit 1
                set ip 10.1.1.1  255.255.255.0
            next
            edit 2
            next
        end
    next
    edit port2
        set vdom root
        set alias no_spaces
        set description no_spaces_descr
        set mtu-override enable
        set mtu 1234
        set vlanid 1236
        set interface port1
        set speed auto
    next
    edit "longest if name"
        set vdom "root"
        set alias ""
        # slash-prefix format
        set ip 169.254.1.1/24
        set status up
        set type vlan
        set vlanid 1235
        set interface port1
        set vrf 31
        set secondary-IP enable
        config secondaryip
            edit 1
                set ip 10.2.1.1/24
            next
        end
        set secondary-IP disable
        set speed 100Gfull
    next
    edit "tunnel"
        set vdom "root"
        set status down
        set type tunnel
        set mtu 65535
    next
    edit "loopback123"
        set vdom "root"
        set type loopback
    next
    edit "agg"
        set vdom "root"
        set type aggregate
    next
    edit "emac"
        set vdom "root"
        set type emac-vlan
    next
    edit "redundant"
        set vdom "root"
        set type redundant
    next
    edit "vlan"
        set vdom "root"
        set type vlan
        set interface port1
        set vlanid 4094
    next
    edit "wl"
        set vdom "root"
        set type wl-mesh
    next
end
