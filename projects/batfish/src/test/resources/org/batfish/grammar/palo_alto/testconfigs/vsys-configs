set deviceconfig system hostname vsys-configs-test

# Vsys configurations
set config devices localhost.localdomain vsys vsys1 display-name Primary-VSYS
set config devices localhost.localdomain vsys vsys1 region us-west-1

# User ID agent configuration
set config devices localhost.localdomain vsys vsys1 user-id-agent serial-number 123456789
set config devices localhost.localdomain vsys vsys1 user-id-agent host user-id-host1
set config devices localhost.localdomain vsys vsys1 user-id-agent host user-id-host2

# ALG configuration
set config devices localhost.localdomain vsys vsys1 alg dns enabled yes
set config devices localhost.localdomain vsys vsys1 alg ftp enabled no

# QoS configuration
set config devices localhost.localdomain vsys vsys1 qos profile qos-profile1 class class1
set config devices localhost.localdomain vsys vsys1 qos profile qos-profile1 class class2

# Threats configuration
set config devices localhost.localdomain vsys vsys1 threats vulnerability rule1 severity critical
set config devices localhost.localdomain vsys vsys1 threats spyware rule2 severity high

# DOS protection
set config devices localhost.localdomain vsys vsys1 dos-protection profile dos-prof1 flood tcp-syn enabled yes

# Email scheduler
set config devices localhost.localdomain vsys vsys1 email-scheduler daily-report schedule daily

# Reports configuration
set config devices localhost.localdomain vsys vsys1 reports traffic-summary type summary
set config devices localhost.localdomain vsys vsys1 reports security-summary type detailed

# Local user database
set config devices localhost.localdomain vsys vsys1 local-user-database user testuser password encryptedpassword

# Settings
set config devices localhost.localdomain vsys vsys1 setting management hostname-type-in-syslog hostname
set config devices localhost.localdomain vsys vsys1 setting wildfire file-size-limit 10
