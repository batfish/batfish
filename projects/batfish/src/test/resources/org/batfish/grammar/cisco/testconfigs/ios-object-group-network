!
hostname ios-object-group-network
!
!
object-group network ogn_prefix
 1.0.0.0 255.0.0.0
!
object-group network ogn_host1
 host 2.0.0.1
!
object-group network ogn_host2
 host 2.0.0.2
!
object-group network ogn_indirect
 group-object ogn_prefix
!
object-group network ogn_network_object
 network-object 2.0.0.1 255.255.255.255
 network-object host 2.0.0.2
 network-object 1.0.0.0 255.0.0.0
!
object-group network ogn_network_object_indirect
 network-object host ogn_host1
 network-object object ogn_host2
 network-object ogn_indirect
!
object-group network ogn_unused
!
!
ip access-list extended acl_prefix
 permit ip object-group ogn_prefix any
!
ip access-list extended acl_host
 permit ip object-group ogn_host1 any
 permit ip object-group ogn_host2 any
!
ip access-list extended acl_indirect
 permit ip object-group ogn_indirect any
!
ip access-list extended acl_undef
 permit ip object-group ogn_undef any
!
