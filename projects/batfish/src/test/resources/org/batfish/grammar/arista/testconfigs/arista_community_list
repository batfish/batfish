!RANCID-CONTENT-TYPE: arista
!
hostname arista_community_list
!
! A standard community-list in the old style (standard is required)
ip community-list standard OLD_CL permit 1:1 GSHUT
! An expanded community-list in the old style (expanded)
ip community-list expanded OLD_ECL deny _3$

! A standard community-list in the new style (standard is absent)
ip community-list NEW_CL deny 459123 internet local-as no-advertise no-export

! An expanded community-list in the old style (regexp)
ip community-list regexp NEW_ECL permit .*

