# RANCID-CONTENT-TYPE: juniper
system {
  host-name "define-structure-from-nested";
}

firewall {
    filter foo {
        /*
         ** Count UDP denies.
         */
        term default-deny-udp {
            from {
                protocol udp;
            }
            then {
                count default-deny-udp;
                discard;
            }
        }
        term other {
            then {
                discard;
            }
        }
    }
}
