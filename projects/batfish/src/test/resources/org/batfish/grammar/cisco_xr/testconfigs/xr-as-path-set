!RANCID-CONTENT-TYPE: cisco-xr
!
hostname xr-as-path-set
!
as-path-set mixed
 # A remark at the top
 dfa-regex '^1_',
 ios-regex '^2_',
 length eq 1,
 length eq 2 all,
 length ge 3,
 length ge 4 all,
 length is 5,
 length is 6 all,
 length le 7,
 length le 8 all,
 # Another remark in the middle
 neighbor-is '1.1 [2..3]',
 neighbor-is '4 [5..6]' exact,
 originates-from '1 [2..3]',
 originates-from '4 [5..6]' exact,
 passes-through '1 [2..3]',
 passes-through '4 [5..6]' exact,
 unique-length eq 1,
 unique-length eq 2 all,
 unique-length ge 3,
 unique-length ge 4 all,
 unique-length is 5,
 unique-length is 6 all,
 unique-length le 7,
 unique-length le 8 all
 # And a remark at the end
end-set
!
end
