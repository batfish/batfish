frr-originator
# This file describes the network interfaces
auto lo
iface lo inet loopback

auto swp1
iface swp1

# ports.conf --
frr version
frr defaults datacenter
!
interface lo
 ip address 1.1.1.1/32
!
interface swp1
 ip address 10.1.1.1/24
!
router bgp 1
 bgp router-id 1.1.1.1
 neighbor 10.1.1.4 remote-as 4
 neighbor 10.1.1.4 route-map MATCH_COMM_OUT out
 network 10.2.2.2/32 route-map SET_COMM
!
ip community-list standard TEST_COMM permit 12345:123
!
route-map SET_COMM permit 20
 set community 12345:123
!
route-map MATCH_COMM_OUT permit 10
 match community TEST_COMM
!
end