frr-originator
# This file describes the network interfaces
auto lo
iface lo inet loopback

auto swp1
iface swp1

auto swp3
iface swp3

# ports.conf --
frr version
frr defaults datacenter
!
interface lo
 ip address 2.2.2.2/32
!
interface swp1
 ip address 10.1.1.2/24
!
interface swp3
 ip address 30.1.1.2/24
!
router bgp 2
 bgp router-id 2.2.2.2
 neighbor 10.1.1.1 remote-as 1
 neighbor 30.1.1.3 remote-as 3
 !
 address-family ipv4 unicast
  neighbor 10.1.1.1 default-originate route-map RM_TEST
  neighbor 30.1.1.3 default-originate route-map RM_TEST
 exit-address-family
!
ip prefix-list PL_DEFAULT seq 5 permit 0.0.0.0/0
!
route-map RM_TEST permit 10
 match ip address prefix-list PL_DEFAULT
 set as-path prepend 23
!
end

