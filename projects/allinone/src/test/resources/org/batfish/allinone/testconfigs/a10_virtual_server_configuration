!BATFISH_FORMAT: a10_acos
!
hostname a10_virtual_server_configuration
!
slb virtual-server vs1 10.10.10.1
  port 443 tcp
    name vs1.10000
    service-group vs1.10000
!
slb service-group vs1.10000 tcp
  member S1 10000
  member S2 9999
!
slb server s1 1.1.1.1
  port 10000 tcp
!
slb server s2 2.2.2.2
  port 9999 tcp
!
end
