{
    "class": "org.batfish.question.edges.EdgesQuestion",
    "differential": false,
    "instance": {
        "description": "Returns Layer 1 links.",
        "instanceName": "layer1Edges",
        "longDescription": "Lists all Layer 1 links in the network.",
        "orderedVariableNames": [
            "nodes",
            "remoteNodes"
        ],
        "tags": [
            "topology"
        ],
        "variables": {
            "nodes": {
                "description": "Include links whose first node matches this name or regex",
                "type": "nodeSpec",
                "value": ".*",
                "displayName": "Nodes"
            },
            "remoteNodes": {
                "description": "Include links whose second node matches this name or regex",
                "type": "nodeSpec",
                "value": ".*",
                "displayName": "Remote Nodes"
            }
        }
    },
    "edgeType": "layer1",
    "nodes": "${nodes}",
    "remoteNodes": "${remoteNodes}"
}
