{
    "class": "org.batfish.question.edges.EdgesQuestion",
    "differential": false,
    "instance": {
        "description": "Returns RIP adjacencies.",
        "instanceName": "ripEdges",
        "longDescription": "Lists all RIP adjacencies in the network.",
        "tags": [
            "topology",
            "rip"
        ],
        "variables": {
            "nodes": {
                "description": "Include adjacencies whose first node matches this name or regex",
                "type": "nodeSpec",
                "value": ".*",
                "displayName": "Nodes"
            },
            "remoteNodes": {
                "description": "Include adjacencies whose second node matches this name or regex",
                "type": "nodeSpec",
                "value": ".*",
                "displayName": "Remote Nodes"
            }
        }
    },
    "edgeType": "rip",
    "nodes": "${nodes}",
    "remoteNodes": "${remoteNodes}"
}
