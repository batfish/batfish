{
    "class": "org.batfish.question.aclreachability2.AclReachability2Question",
    "differential": false,
    "aclNameRegex": "${aclNameRegex}",
    "nodeRegex": "${nodeRegex}",
    "instance": {
        "description": "Display ACLs with unreachable lines",
        "instanceName": "aclReachability",
        "longDescription": "Return ACLs with unreachable lines, along with all of the unreachable lines within the ACL.",
        "tags": [
            "acl",
            "default"
        ],
        "variables": {
            "aclNameRegex": {
                "description": "Only evaluate ACLs whose name matches this regex",
                "type": "javaRegex",
                "value": ".*"
            },
            "nodeRegex": {
                "description": "Only evalute ACLs on nodes whose name matches this regex",
                "type": "nodeSpec",
                "value": ".*"
            }
        }
    }
}
