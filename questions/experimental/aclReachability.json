{
    "class": "org.batfish.question.aclreachability2.AclReachability2Question",
    "differential": false,
    "filterSpecifierFactory": "${filterSpecifierFactory}",
    "filterSpecifierInput": "${filterSpecifierInput}",
    "nodeSpecifierFactory": "${nodeSpecifierFactory}",
    "nodeSpecifierInput": "${nodeSpecifierInput}",
    "instance": {
        "description": "Identifies unreachable lines in ACLs",
        "instanceName": "aclReachability",
        "longDescription": "Report ACLs with unreachable lines, as well as reachability of each line within the ACL. Unreachable lines can indicate erroneous configuration.",
        "tags": [
            "acl",
            "default"
        ],
        "variables": {
            "filterSpecifierFactory": {
              "description": "FilterSpecifierFactory used to process filterSpecifierInput",
              "type": "string",
              "optional": true
            },
            "filterSpecifierInput": {
              "description": "Specifies the filter(s) to be analyzed",
              "type": "string",
              "optional": true
            },
            "nodeSpecifierFactory": {
              "description": "NodeSpecifierFactory used to process nodeSpecifierInput",
              "type": "string",
              "optional": true
            },
            "nodeSpecifierInput": {
              "description": "Only analyze filters on specified nodes",
              "type": "string",
              "optional": true
            }
        }
    }
}
