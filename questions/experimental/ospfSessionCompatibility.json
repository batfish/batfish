{
    "class": "org.batfish.question.ospfsession.OspfSessionCompatibilityQuestion",
    "differential": false,
    "nodes": "${nodes}",
    "remoteNodes": "${remoteNodes}",
    "instance": {
        "description": "Returns compatible OSPF sessions",
        "longDescription": "This question returns compatible OSPF sessions in the network. Answer contains local and remote interfaces, VRFs, IPs and areas for the compatible OSPF sessions.",
        "instanceName": "ospfSessionCompatibility",
         "orderedVariableNames": [
            "nodes",
            "remoteNodes"
        ],
        "tags": [
            "ospf"
        ],
        "variables": {
            "nodes": {
                "description": "Include nodes matching this name or regex",
                "type": "nodeSpec",
                "optional": true,
                "displayName": "Nodes"
            },
             "remoteNodes": {
                "description": "Include remote nodes matching this name or regex",
                "type": "nodeSpec",
                "optional": true,
                "displayName": "Remote Nodes"
            }
        }
    }
}