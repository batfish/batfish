{
    "class": "org.batfish.question.ospfinterface.OspfInterfaceConfigurationQuestion",
    "differential": false,
    "nodes": "${nodes}",
    "properties": "${properties}",
    "instance": {
        "description": "Returns OSPF configuration of interfaces",
        "longDescription": "This question returns the interface level OSPF configuration details. Specifically it lists down, for every interface, its OSPF area, whether it is running OSPF passive, its OSPF cost, its value of OSPF hello mutiplier and also if OSPF point to point is configured.",
        "instanceName": "ospfInterfaceConfiguration",
         "orderedVariableNames": [
            "nodes",
            "properties"
        ],
        "tags": [
            "ospf"
        ],
        "variables": {
            "nodes": {
                "description": "Include nodes matching this name or regex",
                "type": "nodeSpec",
                "optional": true,
                "displayName": "Nodes"
            },
            "properties": {
                "description": "Include properties matching this regex",
                "type": "ospfPropertySpec",
                "optional": true,
                "displayName": "Properties"
            }
        }
    }
}